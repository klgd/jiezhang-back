# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log
/vendor
.byebug_history

config/settings.local.yml
config/settings/*.local.yml
config/environments/*.local.yml
config/database.yml
config/settings/*.yml
nohup.out
# Ignore encrypted secrets key file.
config/secrets.yml
public/uploads
config/environments/*.rb
config/unicorn.rb
.idea
public/letters
# Ignore encrypted secrets key file.
config/secrets.yml.key
private/